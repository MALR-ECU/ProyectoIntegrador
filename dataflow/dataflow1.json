{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "RegistroInspeccionExcel",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "CrearIDUnico",
					"description": "Genera un ID Ãšnico"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(IDUnico = concat(\r",
				"  formatDateTime(toTimestamp(Fecha), 'yyyy'),\r",
				"  formatDateTime(toTimestamp(Fecha), 'MM'),\r",
				"  formatDateTime(toTimestamp(Fecha), 'dd'),\r",
				"  formatDateTime(toTimestamp(Fecha), 'HH'),\r",
				"  formatDateTime(toTimestamp(Fecha), 'mm'), // Minutos\r",
				"  formatDateTime(toTimestamp(Fecha), 'ss')\r",
				")\r",
				") ~> CrearIDUnico",
				"CrearIDUnico sink(validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1) ~> sink1"
			]
		}
	}
}